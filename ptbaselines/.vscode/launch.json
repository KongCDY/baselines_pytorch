    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "ptbaselines.run",
            "args": [
                "--alg", "ppo2",
                "--env", "PongNoFrameskip-v4",
                "--num_timesteps", "1e6",
                "--log_path", "/home/sora/tmp/baselines_test",
                "--save_path", "/home/sora/tmp/baselines_test"
            ]
        },

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--alg",
                "ppo2",
                "--env",
                "PongNoFrameskip-v4",
                "--num_timesteps",
                "1e6"
            ]
        }
    ]
}